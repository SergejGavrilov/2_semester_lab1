#include <iostream>
#include <fstream>
using namespace std;

int lcs(int n, int m, const int* x, const int* y){
    n--;
    m--;
    if (n == 0 || m == 0){
        return 0;
    }
    if (x[n] == y[n]){
        return lcs(n - 1, m - 1, x, y) + 1;
    }
    return max(lcs(n - 1, m, x, y), lcs(n, m - 1, x, y));
}

int main()
{
    int n, m, i;
    ifstream in;
    ofstream out;
    in.open("lcs.in", ios::in);
    out.open("lcs.out", ios::out);
    in << n;
    int x[n];
    for (i = 0; i < n; i++){
        in << x[i];
    }
    in << m;

    int prev[n][m];
    for (i = 0; i < n; i++){
        in << y[i];
    }
    for
    return 0;
}
